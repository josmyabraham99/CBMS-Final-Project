@model IList<ApplicationUser>
@{
    ViewData["Title"] = "CustomerTable";
}

<h1> Customer List</h1>

<table class="table table-striped">
    <tr>
        <th>CustomerName</th>

        <th>Email</th>
        <th>UserType</th>
       
      
       
    </tr>
    @foreach (var movie in Model)
    {
        <tr>
            <td>@movie.FirstName</td>

            <td>@movie.Email</td>
            <td>@movie.LastName</td>
            
               <td> <div class="btn-group ms-auto">
                    
                    <a asp-action="Delete" asp-route-id="@movie.Id" class="btn btn-danger" onclick="return confirm('Do you want to delete this Customer?')">Delete</a>
                </div>
            </td>
           



        </tr>

    }

</table>





